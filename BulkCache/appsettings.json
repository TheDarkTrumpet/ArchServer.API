{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "ConnectionStrings": {
        "EFDbContext": "<MSSQL SERVER HERE>"
    },
    "APISources": {
        "Kimai": {
            "Mysql_CS":"<MYSQL_Connection_String>"
            "FromDateDays": "-1",
            "TimeZone": "America/Chicago"
        },
        "Toggl": {
            "API_Key": "<API_KEY>"
            "Workspace": "<WORKSPACE_NAME>"
        },
        "Teamwork": {
            "API_Key": "<API_KEY>"
            "Base_URL": "<TEAMWORK_URL>"
            "FromDateDays": "-1",
            "IncludeCompleted": "True"
        },
        "VSTS": {
            "API_Key":  "<API_KEY>"
            "Organization": "<ORGANIZATION>"
            "Project": "<PROJECT>"
            "AssignedToInclude": [
                "EMAIL@DOMAIN"
            ],
            "StatesToExclude": [
                "Closed", "Removed", "Done"
            ],
            "TypesToInclude": [
                "Product Backlog Item"
            ],
            "FromDateDays": "-1",
            "IncludeComments": "True"
        }
    }
}

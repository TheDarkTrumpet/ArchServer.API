{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "ConnectionStrings": {
        "EFDbContext": "Server=<SERVER_NAME>;Database=<DATABASE_NAME>;User Id=<UID>;Password=<PASSWORD>"
    },
    "APISources": {
        "Kimai": {
            "Mysql_CS":"server=<SERVER>;Port=<PORT>;userid=<USER>;password=<PASSWORD>;database=<DATABASE>",
            "FromDateDays": "-5",
            "TimeZone": "America/Chicago"
        },
        "Toggl": {
            "API_Key": "<API_KEY>",
            "Workspace": "<WORKSPACE_NAME>"
        },
        "Teamwork": {
            "API_Key": "<API_KEY>",
            "Base_URL": "<BASE_URL>",
            "FromDateDays": "-5",
            "IncludeCompleted": "True"
        },
        "VSTS": {
            "API_Key":  "<API_KEY>",
            "Organization": "<ORGANIZATION_NAME>",
            "Project": "<PROJECT>",
            "AssignedToInclude": [
                "<EMAIL_1>",
                "<EMAIL_2>"
            ],
            "StatesToExclude": [
                "Closed", "Removed", "Done"
            ],
            "TypesToInclude": [
                "Product Backlog Item"
            ],
            "FromDateDays": "-5",
            "IncludeComments": "True"
        }
    }
}
